  
ALGORITHM insertion
VAR

    i , j , temp: INTEGER;
    Tab   : ARRAY_OF type[n];

BEGIN

    FOR i FROM 1 TO tab.lenght-1 STEP 1  DO

        j = i - 1;
        temp = tab[i]

        WHILE (j=>0 and t[j]>temp) DO
            tab[j+1] = tab[j]
            j = j - 1;
        END_WHILE

        tab [j+1] = temp;

    END_FOR
